{
  "description": "IndexTTS2 project configuration. Amharic WebUI available at http://localhost:7863",
  "startupProcesses": [
    {
      "name": "webui",
      "command": "python webui_amharic.py --share",
      "enabled": true,
      "stdoutFile": "logs/webui.log",
      "cwd": "."
    }
  ],
  "fileChangeHooks": [
    {
      "name": "typecheck",
      "command": "python -m py_compile",
      "filePattern": "**/*.py",
      "enabled": false
    }
  ],
  "maxAgentSteps": 50,
  "baseAgent": "codebuff/buffy@latest",
  "addedSpawnableAgents": [
    "codebuff/researcher-web@latest",
    "codebuff/researcher-docs@latest"
  ]
}